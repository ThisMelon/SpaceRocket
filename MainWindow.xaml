<Window x:Class="RocketGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RocketGame"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Image x:Name="Rocket" HorizontalAlignment="Center" Height="162" Margin="0,217,0,0" VerticalAlignment="Top" Width="100" Source="/rocket_off_000.png" Stretch="Fill" StretchDirection="UpOnly"/>
        <Button x:Name="Spin" Content="Spin" HorizontalAlignment="Left" Margin="730,394,0,0" VerticalAlignment="Top" Height="30" Width="60" Click="Spin_Click"/>
        <Button x:Name="Shoot" Content="Shoot" HorizontalAlignment="Left" Margin="665,394,0,0" VerticalAlignment="Top" Height="30" Width="60" Click="Shoot_Click"/>

    </Grid>
</Window>
